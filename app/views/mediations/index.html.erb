<div class="container">
  <div class="section-box">
    <div class="jumbotron">
      <h2 class="display-4"><%= current_user.name %></h2>          
      <hr class="my-4">
    </div>
    <% current_user.mediations.each do |mediation| %>
        <div class="card text-center" style="margin-bottom: 10px">
          <div class="card-body">
            <h5 class="card-title"><%=mediation.protocol_number%></h5>
            <p class="card-text">This card has supporting text below as a natural lead-in to additional content.</p>
            <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
            <%= link_to 'Update', edit_user_mediation_path(id: mediation.id), class:"btn btn-primary" %>
            <a href="#" class="btn btn-danger">Delete</a>
          </div>
        </div>
    <% end %>
  </div>
</div>
  